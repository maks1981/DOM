// console.log(window.document)
// window.document.body.children[0].textContent = 'Hello Girls'
// document.body.children[0].style.color = ' red'
// console.log(document.getElementById('different'))
// console.log(document.querySelector('div'))


 var parent = document.querySelector('.parent');
 var ul = parent.querySelector('ul')
 var div = document.createElement('div');

 div.textContent = 'My first new div';
 div.style.color = 'red'; 
 div.style.fontSize = '20px';
 parent.appendChild(div)
 parent.insertBefore(div,ul);

/*
   so with this below positions we can move our div or p any side

   beforebegin
   afterbegin
   beforeend
   afterend
*/

 parent.insertAdjacentElement('beforeend',div)

parent.insertAdjacentHTML('beforeend','<div>Hey dude!</div>')

 // to delete 

 //parent.removeChild(ul)
  ul.remove();
  
  
